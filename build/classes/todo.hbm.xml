<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="notes.model.Todo" table="todo">
		<id column="ID" name="id" type="int">
			<generator class="increment"></generator>

		</id>
		<property column="USERNAME" name="username" type="string" />
		<property column="TITLE" name="title" type="string" />
		<property column="DESCRIPTION" name="description" type="string" />
		<property column="STATUS" name="status" type="string" />
		<property column="DATE" name="dt" type="date" />
	</class>
</hibernate-mapping>
